// compOut
// line:22 -> 200
// line:23 -> 3
// line:24 -> 25
// line:25 -> 0.5

Number = builtin_Float64
String = builtin_String

operator = @string(String) @left(Number) @right(Number)
	if string == "+" then
		left + right
	else if string == "-" then
		left - right
	else if string == "*" then
		left * right
	else if string == "/" then
		left / right
	else
		0

operator "+" 100 100
operator "-" 5 2
operator "*" 5 5
operator "/" 1 2